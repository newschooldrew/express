<!DOCTYPE html>
<html>
<head>
	<title>	Drew's Movie App </title>
	<% include head %>
</head>
<body>

	<% include navbar %>

	<div class="container">
		<div class="row">
			<div class="col-sm-12">
				<h1></h1>
			</div>
			<div class="col-sm-12">
        <% movieData.forEach((movie)=>{ %>
          <div class="col-md-3 col-sm-4"><%= movie.title %>
         <a href="movie/<%= movie.id %>"><img src="<%= imageBaseUrl + movie.poster_path %>" /></a>
        </div>
        <% }) %>
			</div>
		</div>
	</div>
</body>
</html>